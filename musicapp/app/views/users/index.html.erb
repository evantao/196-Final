<p id="notice"><%= notice %></p>

<table class="table">
  <thead>
    <tr>
      <th>User</th>
      <th>Score </th>
      <th colspan="2"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
	<td><%= user.high_score %></td>
	<td><%= link_to 'Profile', user %></td>
      </tr>
    <% end %>
  </tbody>
</table>

